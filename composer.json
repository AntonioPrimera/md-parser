{
    "name": "antonioprimera/custom-markdown",
    "description": "Custom Markdown Processor",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "AntonioPrimera\\CustomMarkdown\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Antonio Primera",
            "email": "antonio@cus.ro"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.34"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest"
    }
}
